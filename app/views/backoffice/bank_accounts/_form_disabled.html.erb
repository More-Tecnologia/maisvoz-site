<%= f.input :bank_code, collection: form.bank_codes, disabled: true %>
<%= f.input :bank_account_type, collection: form.bank_account_types, disabled: true %>

<div class="form-row">
  <%= f.input :account_number, as: :integer, wrapper_html: { class: 'col-sm-4 p-l-0' }, disabled: true %>
  <%= f.input :account_digit, as: :integer, wrapper_html: { class: 'col-sm-2' }, disabled: true %>
  <%= f.input :agency_number, as: :integer, wrapper_html: { class: 'col-sm-4' }, disabled: true %>
  <%= f.input :agency_digit, as: :integer, wrapper_html: { class: 'col-sm-2 p-r-0' }, disabled: true %>
</div>

<%= f.submit t('save'), class: 'btn btn-default', disabled: true %>
