<!-- Page-Title -->
<div class="row">
  <div class="col-sm-12">

    <h4 class="page-title m-b-30">Meus Ve√≠culos</h4>

  </div>
</div>


<div class="row">
  <div class="col-lg-12">
    <div class="card-box">

      <div class="table-responsive">
        <table class="table table-hover">
          <thead>
            <tr>
              <th>#</th>
              <th>Modelo</th>
              <th>Placa</th>
              <th>Status</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <% @vehicles.each do |vehicle| %>
              <tr>
                <td><%= vehicle.id %></td>
                <td><%= vehicle.car_model.name %></td>
                <td><%= vehicle.plate %></td>
                <td><%= t vehicle.subscription.status %></td>
                <td><%= link_to 'Editar', edit_backoffice_club_motors_vehicle_path(vehicle), class: 'btn btn-sm btn-primary' %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>

  </div> <!-- end col -->

</div>