<div class="row">
  <div class="col-sm-12">
    <div class="panel panel-border panel-custom">
      <div class="panel-heading">
  			<h3 class="panel-title"><%= t('defaults.new_cellphone_reload') %></h3>
  		</div>
      <div class="panel-body">
        <%= search_form_for @q, url: '' do |f| %>
          <div class="form-group">
            <div class="row">
              <div class="col-md-2">
                <%= f.label t('attributes.username') %>
                <%= f.search_field :username_eq, class: 'form-control input-sm' %>
              </div>
            </div>
          </div>
          <%= f.submit t('datagrid.form.search'), class: 'btn btn-default' %>
        <% end %>
      </div>
    </div>
  </div>
</div>
