
<%= stylesheet_link_tag 'click-tok-theme/courses' %>

<div class="main-box">
  <%= render 'menu' %> 
  <%= render 'banner',  badge_pretext_image: 'stores/banner-badge.svg',
                      date: '13/04/2022',
                      description: 'Short description',
                      pretext: 'Últimos Numeros',
                      price: '$20.00',
                      product_image: 'stores/banner-badge.svg',
                      title: 'Nome de um produto' %>
  <div id="highlights" class="container-online-courses">
    <span class="title-courses">
      <%= t(:featured_courses) %>
    </span>
    <div class="cards-courses">
      <% @courses.each do |course| %>
        <div class="card-course">
          <%= link_to course_backoffice_store_path(course) do %>
            <img class="img-card-course" src="<%= course_thumb(course) %>">
          <% end %>
          <div class="bottom-card-course">
            <div class="container-created-by">
              <span class="created-by"><%= t(:published_by) %></span>
              <span class="author-course"><%= course.owner_name %></span>
            </div>
            <%= render 'layouts/shared/reputation' %>
            <div class="title-card-course">
              <p>
                <%= course.short_description %>
              </p>
            </div>
            <div class="buy-course">
              <span class="value-course"><%= number_to_currency(course.price) %></span>
              <%= link_to t(:buy), backoffice_courses_carts_path(product: { id: course.product.id }, country: current_user.country), method: :put, class: "btn-buy-course" unless course.in?(current_user.courses) %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
