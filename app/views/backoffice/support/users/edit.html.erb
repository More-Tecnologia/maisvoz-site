<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
<!-- Page-Title -->
<div class="box-orders">
  <div class="box-second">
    <h4 class="page-title"><%= user.name %></h4>
    <p class="text-muted page-title-alt"><%= t(:user_edit_form) %></p>
  </div>
</div>

<div class="box-orders">
  <div class="box-second">
    <div class="card-box">
      <div class="row">
        <div class="col-lg-12">
          <div class="p-20">
            <%= render partial: 'form', locals: { user_form: user_form, url: backoffice_support_user_path(user), method: :put } %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
