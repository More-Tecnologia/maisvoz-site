<!-- Page-Title -->
<div class="row">
  <div class="col-sm-12">
    <h4 class="page-title"><%= user.name %></h4>
    <p class="text-muted page-title-alt"><%= t('views.backoffice.admin.products.edit.subtitle') %></p>
  </div>
</div>

<div class="row">
  <div class="col-sm-12">
    <div class="card-box">
      <div class="row">
        <div class="col-lg-12">
          <div class="p-20">
            <%= render partial: 'form', locals: { user_form: user_form, url: backoffice_support_user_path(user), method: :put } %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
