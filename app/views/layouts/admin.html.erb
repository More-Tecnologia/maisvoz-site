<!DOCTYPE html>
<html>
  <head>
    <%= display_meta_tags site: ENV['COMPANY_NAME'] %>
    <%= csrf_meta_tags %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta name="theme-color" content="#3F3F3F" />
    <%= render 'layouts/shared/favicons' %>
    <%= stylesheet_link_tag 'click-tok-theme/app' %>
    <%= render 'layouts/shared/assets' %>
  <body>
    <div id="root">
      <div class="disable-of">
        <div class="container align">
          <div class="main-app">
            <%= render 'layouts/top_bar' %>
            <div class="main-app-content">
              <% if current_user.refused_verification? %>
                <div class="alert alert-danger"><%= current_user.document_refused_reason %></div>
              <% end %>
              <%= render 'layouts/flash-messages' %>
              <%= render 'layouts/notification' %>
              <%= yield %>
              <div class="t">
              </div>
              <div class="t">
              </div>
            </div>
            <%= render 'layouts/shared/footer' %>
          </div>
        </div>
      </div>
    </div>
    <%= render 'layouts/gtag' %>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.1/css/all.css" integrity="sha384-gfdkjb5BdAXd+lj+gudLWI+BXq4IuLW5IT+brZEZsLFm++aCMlF1V92rMkPaX4PP" crossorigin="anonymous">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/printThis/1.15.0/printThis.min.js"></script>
  </body>
</html>
