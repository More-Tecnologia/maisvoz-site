<!DOCTYPE html>
<html>
  <head>
    <%= display_meta_tags site: 'Future Motors' %>
    <%= csrf_meta_tags %>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="wrapper">
      <%= render 'layouts/public/topbar' %>
      <div class="content-page no-margin">
        <div class="content">
          <div class="container">
            <%= render 'layouts/flash-messages' %>
            <%= yield %>
          </div>
        </div>
        <%= render 'layouts/public/footer' %>
      </div>
    </div>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= render 'layouts/gtag' %>
  </body>
</html>
