<%= stylesheet_link_tag 'click-tok-theme/simple_header' %>

<div class="simple-header">
  <%= render 'layouts/shared/language_select' %>
  <div class="simple-header-logo">
    <% if ActiveModel::Type::Boolean.new.cast(ENV['WHITELABEL']) %>
      <%= image_tag('white-label-logo.png', alt: SystemConfiguration.company_name, 'stroke-width' => '2' ) %>
    <% else %>
      <%= image_tag('logo.png', alt: SystemConfiguration.company_name, 'stroke-width' => '2' ) %>
    <% end %>
  </div>
</div>
